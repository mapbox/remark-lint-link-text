{
  "name": "@mapbox/remark-lint-link-text",
  "version": "0.4.5",
  "description": "A remark-lint plugin that warns against non-descriptive link text.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "lint": "eslint ."
  },
  "author": "Mapbox",
  "license": "MIT",
  "dependencies": {
    "unified-lint-rule": "^1.0.4",
    "unist-util-visit": "^1.4.1"
  },
  "devDependencies": {
    "dedent": "^0.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^8.0.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "remark": "^10.0.1"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "remark-lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/remark-lint-link-text.git"
  },
  "bugs": {
    "url": "https://github.com/mapbox/remark-lint-link-text/issues"
  },
  "homepage": "https://github.com/mapbox/remark-lint-link-text#readme"
}
