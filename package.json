{
  "name": "@mapbox/remark-lint-link-text",
  "version": "0.6.0",
  "description": "A remark-lint plugin that warns against non-descriptive link text.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "author": "Mapbox",
  "license": "MIT",
  "dependencies": {
    "unified-lint-rule": "^1.0.6",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@mapbox/prettier-config-docs": "^2.0.0",
    "dedent": "^0.7.0",
    "eslint": "^8.5.2",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.4",
    "prettier": "^2.2.1",
    "remark": "^13.0.0"
  },
  "prettier": "@mapbox/prettier-config-docs",
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "remark-lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/remark-lint-link-text.git"
  },
  "bugs": {
    "url": "https://github.com/mapbox/remark-lint-link-text/issues"
  },
  "homepage": "https://github.com/mapbox/remark-lint-link-text#readme"
}
